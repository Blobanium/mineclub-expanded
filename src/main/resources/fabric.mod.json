{
  "schemaVersion": 1,
  "id": "mineclubexpanded",
  "version": "${version}",

  "name": "Mineclub Expanded",
  "description": "This is a fabric mod that enhances mineclub",
  "authors": [
    "Blobanium"
  ],
  "contact": {
    "issues": "https://github.com/Blobanium/mineclub-expanded/issues",
    "sources": "https://github.com/Blobanium/mineclub-expanded"
  },

  "license": "MIT",
  "icon": "assets/mineclubexpanded/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "io.github.blobanium.mineclubexpanded.MineclubExpanded"
    ],
    "modmenu": [
      "io.github.blobanium.mineclubexpanded.util.config.ModMenuConfig"
    ]
  },
  "mixins": [
    "mineclubexpanded.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": ["1.17.x", "1.18.x"],
    "java": ">=16"
  },
  "recommends": {
    "fabricloader": ">=0.12.9"
  },
  "conflicts": {
    "advancedchat": ["*"]
  },
  "breaks": {
   "autoreconnect": ["*"]
  },
  "custom": {
    "modmenu":{
      "links": {
        "Discord (Mineclub Beehive)": "https://discord.gg/uQhX9mNAxd",
        "Discord (Development Server)": "https://discord.gg/KfNqUs2uq5",
        "Donate": "https://gist.github.com/Blobanium/190179ae9ee3d00f42aad2a86c37625c"
      }
    }
  }
}
